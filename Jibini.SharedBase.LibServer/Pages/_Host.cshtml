@page "/"
@namespace Jibini.SharedBase.Server.Pages
@using Microsoft.AspNetCore.Components.Web
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = "_Layout";
}

<component type="@App.StartupType" render-mode="ServerPrerendered" />

@section HeadContent {
    <component type="typeof(HeadOutlet)" render-mode="ServerPrerendered" />

    <link href="https://cdn.jsdelivr.net/npm/bootswatch@5.2.0/dist/yeti/bootstrap.min.css" rel="stylesheet" crossorigin="anonymous">
    
    <link href="_content/Havit.Blazor.Components.Web.Bootstrap/defaults.css" rel="stylesheet" />
}

@section ScriptContent {
    <script src="https://cdn.jsdelivr.net/npm/chart.js@2.9.4/dist/Chart.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.29.1/moment.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/js/bootstrap.bundle.min.js" crossorigin="anonymous"></script>

    <script src="_content/ChartJs.Blazor/ChartJsBlazorInterop.js"></script>
    <script src="_content/Jibini.SharedBase.Components/js/IframeDownloadInterop.js"></script>

    <script src="_framework/blazor.server.js"></script>
}